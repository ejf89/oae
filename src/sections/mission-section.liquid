<!-- about-section.liquid -->


  <ul class='mission-link-container med-font'>
    <li>
      <p class='color-green med-font'>Philanthropic Partners</p>
    </li>
  {%- for block in section.blocks -%}
  <li>
    <a class="color-black" href="{{blocks.settings.link_url}}" target="_blank">{{block.settings.link_copy}}</a>
  </li>

  {%- endfor -%}
</ul>







{% schema %}
  {
    "name": "Mission Section",
    "class": "index-section",
    "settings": [
      {
        "type": "text",
        "label": "Header Copy",
        "id": "header_copy"
      }
    ],
    "blocks": [
      {
        "type": "link_block",
        "name": "link",
        "settings": [
          {
            "type": "text",
            "id": "link_copy",
            "label": "Link Copy"
          },
          {
            "type": "url",
            "id": "link_url",
            "label": "URL"
          }
        ]
      }
    ]
  }
{% endschema %}
